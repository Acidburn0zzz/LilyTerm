#: console.c:121
#, c-format
msgid "Please report bug at <%s>.\n"
msgstr "Veuillez s'il vous plaît reporter les bogues à %s.\n"

#: console.c:122
#: dialog.c:861
#, c-format
msgid "Thank you for using %s!"
msgstr "Merci d'utiliser %s !"

#: console.c:140
#, c-format
msgid ""
"%s is a libvte based X Terminal Emulator.\n"
"\n"
msgstr ""
"%s est un émulateur de terminal basé sur libvte.\n"
"\n"

#: console.c:141
msgid "Use -e/-x/--execute {Command} to run a command when starting up. (Must be the final option).\n"
msgstr "Utilisez -e/-x/--execute {commande} pour exécuter une commande au lancement (doit être la dernière option).\n"

#: console.c:143
msgid "Use -T/--title {title} to specify the window title.\n"
msgstr "Utilisez -T/--title {titre} pour modifier le titre de la fenêtre.\n"

#: console.c:144
msgid "Use -t/--tab {number} to open multi tabs when starting up.\n"
msgstr "Utilisez -t/--tab {nombre} pour ouvrir plusieurs onglets au démarrage.\n"

#: console.c:145
msgid "Use -d/--directory {directory} to specify the init directory when starting up.\n"
msgstr "Utilisez -d/--directory {répertoire} pour modifier le répertoire courant.\n"

#: console.c:146
msgid "Use -g/--geometry {WIDTHxHEIGHT[+-]XOFFSET[+-]YOFFSET} to specify the geometry of window.\n"
msgstr "Utilisez -g/--geometry {LARGEURxHAUTEUR[+-]DÉCALAGE_X[+-]DÉCALAGE_Y} pour spécifier la géométrie de la fenêtre.\n"

#: console.c:147
msgid "Use -l/-ls/--login to make the shell invoked as a login shell.\n"
msgstr "Utilisez -l/-ls/--login pour invoquer le shell en tant que shell de login.\n"

#: console.c:149
msgid "Use -s/--separate to run in separate process.\n"
msgstr "Utilisez -s/--separate pour utiliser un processus séparé.\n"

#: console.c:150
msgid "Use -v/--version to show the version information.\n"
msgstr "Utilisez -v/--version pour afficher les informations de version.\n"

#: console.c:151
msgid "Use -p/--profile to got a profile sample.\n"
msgstr "Utilisez -p/--profile pour créer un exemple de profil.\n"

#: console.c:153
#, c-format
msgid ""
"Use -u/--user_profile {%s} to use a specified profile.\n"
"\n"
msgstr ""
"Utilisez -u/--user_profile {%s} pour utiliser le profil spécifié.\n"
"\n"

#: console.c:159
