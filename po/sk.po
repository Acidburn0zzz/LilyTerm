#: console.c:116 dialog.c:835
msgid ""
"Default shortcut key: (It may custom or disable by right click menu [Set "
"function key value])"
msgstr ""
"Predvolené klávesové skratky: (Môže byť vlastná alebo zakázaná pomocou "
"kontextového menu [Nastaviť funkčné klávesy])"

#: console.c:124
#, c-format
msgid ""
"\n"
"Please report bug to %s. Thank you for using %s!"
msgstr ""
"\n"
"Prosím chyby hláste na %s. Ďakujem, že používate %s!"

#: console.c:143
#, c-format
msgid ""
"%s is a libvte based X Terminal Emulator.\n"
"\n"
msgstr ""
"%s je emulátor terminálu pre X založený na libvte.\n"
"\n"

#: console.c:144
msgid ""
"Use -e/-x/--execute {Command} to run a command when starting up. (Must be "
"the final option).\n"
msgstr ""
"Použite -e/-x/--execute {Príkaz} na spustenie príkazu pri štarte. (Musí byť "
"ako posledná voľba).\n"

#: console.c:146
msgid "Use -T/--title {title} to specify the window title.\n"
msgstr "Použite -T/--title {titulok} na nastavenie titulku okna.\n"

#: console.c:147
msgid "Use -t/--tab {number} to open multi tabs when starting up.\n"
msgstr "Použite -t/--tab {číslo} na otvorenie viacerých záložiek pri štarte.\n"

#: console.c:148
msgid ""
"Use -d/--directory {directory} to specify the init directory when starting "
"up.\n"
msgstr ""
"Použite -d/--directory {adresár} na nastavenie počiatočného adresára pri "
"štarte.\n"

#: console.c:149
#, fuzzy
#| msgid "Use -g/--geometry {geometry} to specify the geometry of window.\n"
msgid ""
"Use -g/--geometry {WIDTHxHEIGHT[+-]XOFFSET[+-]YOFFSET} to specify the "
"geometry of window.\n"
msgstr "Použite -g/--geometry {geometria} na nastavenie geometrie okna.\n"

#: console.c:150
msgid "Use -l/-ls/--login to make the shell invoked as a login shell.\n"
msgstr ""
"Použite -l/-ls/--login na spustenie shellu ako prihlasovacieho shellu.\n"

#: console.c:152
msgid "Use -s/--separate to run in separate process.\n"
msgstr "Použite -s/--separate na spustenie v samostatnom procese.\n"

#: console.c:153
msgid "Use -v/--version to show the version information.\n"
msgstr "Použite -v/--version na zobrazenie informácií o verzii.\n"

#: console.c:154
msgid "Use -p/--profile to got a profile sample.\n"
msgstr "Použite -p/--profile na získanie ukážkového profilu.\n"

#: console.c:156
#, c-format
msgid ""
"Use -u/--user_profile {%s} to use a specified profile.\n"
"\n"
msgstr ""
"Použite -u/--user_profile {%s} na použitie zadaného profilu.\n"
"\n"

#: console.c:162
#, fuzzy, c-format
#| msgid "The %s system config is: %s/%s\n"
msgid "The %s system configure file is: %s/%s\n"
msgstr "Systémová konfigurácia %s je: %s/%s\n"

#: console.c:199
#, c-format
msgid "And your %s profile is: "
msgstr "A váš %s profil je: "

#: console.c:201
#, c-format
