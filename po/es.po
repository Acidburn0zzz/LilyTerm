#: console.c:141
msgid "Use -T/--title {title} to specify the window title.\n"
msgstr "Use -T/--title {título} para especificar el título de la ventana.\n"

#: console.c:142
msgid "Use -t/--tab {number} to open multi tabs when starting up.\n"
msgstr "Use -t/--tab {número} para abrir múltiples solapas al comienzo.\n"

#: console.c:143
msgid ""
"Use -n/--tab_names {tab names} to specify tab names, separate with <space>.\n"
msgstr ""

#: console.c:144
msgid ""
"Use -d/--directory {directory} to specify the init directory when starting "
"up.\n"
msgstr ""
"Use -d/--directory {directorio} para especificar el directorio donde se "
"ubicará inicialmente.\n"

#: console.c:145
#, fuzzy
#| msgid "Use -T/--title {title} to specify the window title.\n"
msgid ""
"Use -g/--geometry {WIDTHxHEIGHT[+-]XOFFSET[+-]YOFFSET} to specify the "
